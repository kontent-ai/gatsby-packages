{
  "name": "@kentico/gatsby-starter-kontent-hello-world",
  "private": true,
  "description": "A simplified bare-bones testing site for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kentico/gatsby-source-kontent"
  },
  "bugs": {
    "url": "https://github.com/Kentico/gatsby-source-kontent/issues"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:preview": "cross-env  ENABLE_GATSBY_REFRESH_ENDPOINT=true gatsby develop",
    "lint": "eslint --ignore-path .gitignore \"**/*.{js,jsx}\"",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"No tests written! -> https://gatsby.dev/unit-testing\" && exit 0"
  },
  "dependencies": {
    "@kentico/kontent-delivery": "^10.3.0",
    "gatsby": "4.0.0",
    "gatsby-plugin-image": "^1.0.0",
    "qs": "^6.9.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rxjs": "^6.5.5"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-react-app": "^6.0.0",
    "prettier": "^1.19.1",
    "babel-eslint": "^10.1.0"
  }
}
